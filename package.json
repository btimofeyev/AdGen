{
    "name": "ad-wizard",
    "version": "1.0.0",
    "description": "Automated ad generation tool using AI",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && node index.js",
        "client": "cd client && npm start",
        "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "keywords": [
        "openai",
        "image-generation",
        "ad-creator"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "concurrently": "^8.2.1",
        "node-cron": "^3.0.3",
        "react-router-dom": "^7.5.2"
    }
}
